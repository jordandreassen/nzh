{
	"manifest_version": 2,
	"name": "NZHerald Premium Viewer",
	"version": "0.0.3",
	"content_scripts": [
		{
			"matches": ["https://*.nzherald.co.nz/*"],
			"js": ["jquery.min.js", "contentScript.js"]
		}
	],
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://www.nzherald.co.nz/arc/subs/p.js"
	],
	"description": "Allows you to view nzherald premium articles for free"
}